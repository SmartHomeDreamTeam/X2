<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="X2.Domain"
                   namespace="X2.Domain"
                   auto-import ="false">
  <class name="X2.Domain.Entity.EventLog" table="EventLog" lazy="true">
    <id name="ID" column ="EventLogID">
      <generator class="guid.comb" />
    </id>

    <property name="UserInforID" type="Guid"/>
    <property name="LogID" type="Guid"/>
    <property name="Description" type="String"/>
  </class>

</hibernate-mapping>
